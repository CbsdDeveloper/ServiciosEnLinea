{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "amd": true,
    "mocha": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "semi": 1,
    "eqeqeq": 1,
    "quotes": [1, "single"],
    "indent": [1, 2, {"SwitchCase": 1}],
    "no-trailing-spaces": 1,
    "camelcase": 1,
    "comma-dangle": 1,
    "curly": 1
  },
  "globals": {
    "angular": true,
    "L": true,
    "lvector": true
  }
}
